{
  "name": "sveltr",
  "version": "1.0.0",
  "description": "Sveltr is a blogdown boilerplate for creating truly epic visualizations!",
  "repository": "dancingfrog/sveltr",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:_rollup && npm run build:blogdown",
    "build:preview": "npm run build:_rollup && npm run build:_preview",
    "build:blogdown": "Rscript src/build.R; echo 'done'",
    "build:_preview": "Rscript src/build_preview.R; echo 'done'",
    "build:_rollup": "rollup -c",
    "build:_webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "install-extra:fedora": "sudo dnf install pandoc gdal-devel geos-devel proj-devel udunits2-devel",
    "lint": "eslint src",
    "live-preview": "run-p live-preview:**",
    "live-preview:_webpack": "webpack --config webpack.dev.js -w",
    "live-preview:blogdown": "npm run start -- ",
    "prebuild": "rimraf dist",
    "postinstall": "Rscript src/install.R",
    "start": "Rscript src/start.R; echo 'servr has stopped'",
    "start:dev": "rollup -c -w"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.0",
    "@sveltejs/gl": "git+https://github.com/Real-Currents/SvelteGL.git#real/currents",
    "acorn": "^7.1.1",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.18.1",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^4.1.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.30.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.2.0",
    "rollup-plugin-shader": "0.0.3",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "sass-loader": "^8.0.0",
    "serialize-javascript": "^2.1.1",
    "style-loader": "^1.0.0",
    "svelte": "^3.19.1",
    "uglifyjs-webpack-plugin": "git+https://github.com/Real-Currents/uglifyjs-webpack-plugin.git#real/currents",
    "url-loader": "^2.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
